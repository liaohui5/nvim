{
	"biome config generator": {
		"prefix": "biomeconfig",
		"body": [
			"{",
			"  \"formatter\": {",
			"    \"enabled\": true,",
			"    \"formatWithErrors\": false,",
			"    \"ignore\": [],",
			"    \"attributePosition\": \"auto\",",
			"    \"indentStyle\": \"space\",",
			"    \"indentWidth\": 2,",
			"    \"lineEnding\": \"lf\",",
			"    \"lineWidth\": 120",
			"  },",
			"  \"javascript\": {",
			"    \"formatter\": {",
			"      \"arrowParentheses\":\"always\",",
			"      \"bracketSameLine\": false,",
			"      \"bracketSpacing\": true,",
			"      \"jsxQuoteStyle\": \"double\",",
			"      \"quoteProperties\": \"asNeeded\",",
			"      \"semicolons\": \"always\",",
			"      \"trailingCommas\": \"all\"",
			"    }",
			"  },",
			"  \"json\": {",
			"    \"formatter\": {",
			"      \"trailingCommas\": \"none\"",
			"    }",
			"  }",
			"}"
